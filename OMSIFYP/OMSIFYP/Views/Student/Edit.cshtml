@model OMSIFYP.Models.Student

@{
    ViewBag.Title = "Edit";
}

<link type="text/css" rel="stylesheet" href="~/css/bootstrap.min.css">
<link type="text/css" rel="stylesheet" href="~/css/multilpe-select.css">
<link type="text/css" rel="stylesheet" href="~/css/style.css">

<style>
    input[type=checkbox] {
        transform: scale(1.5);
    }
</style>

<div align="center"><p><h2>Edit Student</h2></p></div>
<script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
<script type="text/javascript">
        $(document).ready(function () {

       $("#CNIC").keyup(function (e)
       {

          if (e.keyCode != 15) {
              if ($(this).val().length ==5) {
                  $(this).val($(this).val() + "-");
              }
               else if ($(this).val().length ==13) {
                  $(this).val($(this).val()+"-");

               }

          }
       });
       $("#fCNIC").keyup(function (e) {

           if (e.keyCode != 15) {
               if ($(this).val().length == 5) {
                   $(this).val($(this).val() + "-");
               }
               else if ($(this).val().length == 13) {
                   $(this).val($(this).val() + "-");

               }

           }
       });
       $("#pho").keyup(function (e) {

           if (e.keyCode !=13) {
               if ($(this).val().length == 4) {
                   $(this).val($(this).val() + "-");
               }


           }
       });
   });

</script>



@using (Html.BeginForm("Edit", "Student", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
