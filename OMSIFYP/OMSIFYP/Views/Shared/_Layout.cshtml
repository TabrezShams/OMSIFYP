    <header class="topbar clearfix">
      
        <!--Topbar Left Branding With Logo Start-->
        <div class="topbar-left pull-left">
            <div class="clearfix">
                <ul class="left-branding pull-left clickablemenu ttmenu dark-style menu-color-gradient">
                    <li><span class="left-toggle-switch"><i class="zmdi zmdi-menu"></i></span></li>
                    <li>
                        <div class="logo">
                            <a href="#" title="Admin Template"><img src="~/images/mylogo.png" alt="logo"></a>
                        </div>
                    </li>
                </ul>
                <!--Mobile Search and Rightbar Toggle-->

            </div>
        </div>
        <!--Topbar Left Branding With Logo End-->
        <!--Topbar Right Start-->
        <div class="topbar-right pull-right">
            <div class="clearfix">
                <!--Mobile View Leftbar Toggle-->
                <ul class="left-bar-switch pull-left">
                    <li><span class="left-toggle-switch"><i class="zmdi zmdi-menu"></i></span></li>
                </ul>
            </div>
        </div>
        <!--Topbar Right End-->
    </header>

    @*--------------------------------*@

    <!--Leftbar Start Here-->
    <aside class="leftbar">
        <div class="left-aside-container">
            <div class="user-profile-container">
                <div class="user-profile clearfix">
                    <div class="admin-user-thumb">
                        <a href="#"> <img src="@Session["userImg"].ToString()" alt="admin"></a>
                    </div>
                    <div class="admin-user-info">
                        <ul>
                            <li><a href="#">@Session["userName"].ToString()</a></li>
                            <li><a href="#">@Session["userEmail"].ToString()</a></li>

                        </ul>
                    </div>
                </div>
                <div class="admin-bar">
                    <ul>
                        <li>@Html.ActionLink("Logout", "Login", "Login")</li>
       
                    </ul>
                </div>
            </div>
            <ul class="list-accordion tree-style">
                <li class="list-title">Classes</li>
                <li>
                    <a href="#"><i class="zmdi zmdi-view-dashboard"></i><span class="list-label">Classes Data</span></a>
                    <ul>
                        <li>@Html.ActionLink("Generate New Class", "Create", "GenrateClasses")</li>
                        <li>@Html.ActionLink("View Classes", "Index", "GenrateClasses")</li>
                    </ul>
                </li>
                <li class="list-title">Courses</li>
                <li>
                    <a href="#"><i class="zmdi zmdi-view-dashboard"></i><span class="list-label">Courses Data</span></a>
                    <ul>
                        <li>@Html.ActionLink("Add new Course", "Create", "Course")</li>
                        <li>@Html.ActionLink("View Courses", "Index", "Course")</li>
                    </ul>
                </li>
                <li class="list-title">Departments</li>
                <li>
                    <a href="#"><i class="zmdi zmdi-view-dashboard"></i><span class="list-label">Departments Data</span></a>
                    <ul>
                        <li>@Html.ActionLink("Add New Department", "Create", "Department")</li>
                        <li>@Html.ActionLink("View Departments", "Index", "Department")</li>
                    </ul>
                </li>

                <li class="list-title">Instructor</li>
                <li>
                    <a href="#"><i class="zmdi zmdi-view-dashboard"></i><span class="list-label">Instructor Data</span></a>
                    <ul>
                        <li>@Html.ActionLink("Add new Instructor","Create","Instructor")</li>
                        <li>@Html.ActionLink("View Instructors", "Index", "Instructor")</li>
                        
                    </ul>
                </li>

                <li class="list-title">Messages</li>
                <li>
                    <a href="#"><i class="zmdi zmdi-view-web"></i><span class="list-label">Messages</span></a>
                    <ul>
                        <li>@Html.ActionLink("Create Message", "Create", "Message")</li>
                        <li>@Html.ActionLink("View Messages", "Index", "Message")</li>

                    </ul>
                </li>
                <li class="list-title">PTM</li>
                <li>
                    <a href="#"><i class="zmdi zmdi-view-web"></i><span class="list-label">PTM Data</span></a>
                    <ul>
                        <li>@Html.ActionLink("Annouce PTM", "CreatebyAdmin", "PTMCalls")</li>
                        <li>@Html.ActionLink("View PTM", "Index", "PTMCalls")</li>

                    </ul>
                </li>

                <li class="list-title">Students</li>
                <li>
                    <a href="#"><i class="zmdi zmdi-view-web"></i><span class="list-label">Student's Data</span></a>
                    <ul>
                        <li>@Html.ActionLink("Add new Student", "Create", "Student")</li>
                        <li>@Html.ActionLink("View All Student", "Index", "Student")</li>

                    </ul>
                </li>
          
            </ul>
        </div>
    </aside>
    <!--Leftbar End Here-->



    <section class="main-container">
        <div class="container-fluid">
           
            <div class="row">
                <div class="col-md-2">

                </div>
                <div class="col-md-8">
                    @RenderBody()
                </div>
                
                <div class="col-md-2">

                </div>
                </div>
            </div>
        
        <!--Footer Start Here -->
        
        <!--Footer End Here -->
        </section>


